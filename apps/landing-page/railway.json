{
   "$schema": "https://railway.com/railway.schema.json",
   "build": {
      "builder": "DOCKERFILE",
      "watchPatterns": ["/apps/landing-page/**"],
      "dockerfilePath": "apps/landing-page/Dockerfile"
   },
   "deploy": {
      "runtime": "V2",
      "numReplicas": 1,
      "sleepApplication": false,
      "multiRegionConfig": {
         "us-east4-eqdc4a": {
            "numReplicas": 1
         }
      },
      "restartPolicyType": "ON_FAILURE",
      "restartPolicyMaxRetries": 10
   }
}

         },
         "openrouter": {
            "@openrouter/ai-sdk-provider": "1.1.2",
            "ai": "5.0.5"
         },
         "workers": {
            "bullmq": "5.58.7",
            "ioredis": "5.7.0"
         }
      },
      "packages": [
         "tooling/*",
